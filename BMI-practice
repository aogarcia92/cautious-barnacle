import java.util.*;
public class Main {  public static void main(String[] args) {        float height_inches, weight_pounds, height_meters, weight_kilos, bmi;        Scanner sc = new Scanner(System.in);    System.out.println("What is your height in inches?");    height_inches = Float.parseFloat(sc.nextLine());    System.out.println("What is your weight in pounds?");    weight_pounds = Float.parseFloat(sc.nextLine());    System.out.println("Your height in inches: " + height_inches);    System.out.println("Your weight in pounds: " + weight_pounds);        height_meters = (height_inches * 2.54f) / 100;    weight_kilos = weight_pounds * 0.453592f;    bmi = weight_kilos / (float)Math.pow(height_meters, 2);        System.out.println("Congratulations, your BMI is: " + bmi + " meaning you are " + bmiLevels(bmi));      }    public static String bmiLevels(float b) {    if(b < 0) {      return "Error.";    }    if(b < 15.0) {      return "possibly a member of the underground army.";    } else if(b < 16.1) {        return "eating air on a daily basis.";    } else if(b < 18.5) {        return "probably a model giving up the good food.";    } else if(b < 25.0) {        return "basic af.";    } else if(b < 30.0) {        return "getting on the chunky side. Could be THICC tho.";    } else if(b < 35.0) {        return "definitely chunky; even THICCness has a limit.";    } else if(b < 40.0) {        return "like a well-layered muffin, or I guess cupcake with EXTRA icing and EXTRA sprinkles.";    } else {    return "definitely a cake, like a wedding cake, a REALLY, REALLY large wedding cake... you ain't even thicc anymore...";    }    /**public static String bmiLevels(float b) {    if(b < 0) {      return "Error.";    }    if(b < 15.0) {      return "very severely underweight";    } else if(b < 16.1) {        return "severely underweight";    } else if(b < 18.5) {        return "underweight";    } else if(b < 25.0) {        return "normal weight";    } else if(b < 30.0) {        return "overweight";    } else if(b < 35.0) {        return "moderately obese";    } else if(b < 40.0) {        return "severely obese";    } else {    return "morbidly obese";    }    */  }}
